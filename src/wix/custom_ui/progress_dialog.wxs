<Wix
    xmlns="http://wixtoolset.org/schemas/v4/wxs">

    <Fragment>
        <UI>

            <Dialog
                CustomPalette="no"
                ErrorDialog="no"
                Height="270"
                Hidden="no"
                Id="ProgressDialog"
                KeepModeless="no"
                LeftScroll="no"
